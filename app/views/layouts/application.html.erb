<!DOCTYPE html>
<html>
<head>
  <title>BsScheduler</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

	<% if logged_in? %>
		<%= link_to " #{current_user.id} #{current_user.name}", edit_user_path(current_user) %> <%= link_to "Profile", current_user %> <%= link_to "Log out", logout_path, method: "delete" %>
	<% else %>
			<%= link_to "Log in", login_path %>
	<% end %>



	<%= yield %>
	<%= debug(params) if Rails.env.development? %>

</body>
</html>
